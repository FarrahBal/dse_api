{
  "type": "collection",
  "name": "DSE API",
  "version": "1.0",
  "requests": [
    {
      "_id": "req-register",
      "name": "Auth - Register",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/register",
      "headers": [
        { "key": "Content-Type", "value": "application/json" }
      ],
      "body": {
        "type": "json",
        "raw": "{\n  \"name\": \"Test User\",\n  \"email\": \"test@example.com\",\n  \"password\": \"pass123\"\n}"
      }
    },
    {
      "_id": "req-login",
      "name": "Auth - Login",
      "method": "POST",
      "url": "{{baseUrl}}/api/auth/login",
      "headers": [
        { "key": "Content-Type", "value": "application/json" }
      ],
      "body": {
        "type": "json",
        "raw": "{\n  \"email\": \"test@example.com\",\n  \"password\": \"pass123\"\n}"
      }
    },
    {
      "_id": "req-create-item",
      "name": "Items - Create",
      "method": "POST",
      "url": "{{baseUrl}}/api/items",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Authorization", "value": "Bearer {{token}}" }
      ],
      "body": {
        "type": "json",
        "raw": "{\n  \"title\": \"Buy milk\",\n  \"description\": \"2 liters\"\n}"
      }
    },
    {
      "_id": "req-get-items",
      "name": "Items - Get All",
      "method": "GET",
      "url": "{{baseUrl}}/api/items",
      "headers": []
    },
    {
      "_id": "req-update-item",
      "name": "Items - Update",
      "method": "PUT",
      "url": "{{baseUrl}}/api/items/{{itemId}}",
      "headers": [
        { "key": "Content-Type", "value": "application/json" },
        { "key": "Authorization", "value": "Bearer {{token}}" }
      ],
      "body": {
        "type": "json",
        "raw": "{\n  \"title\": \"Updated title\",\n  \"description\": \"Updated description\"\n}"
      }
    },
    {
      "_id": "req-delete-item",
      "name": "Items - Delete",
      "method": "DELETE",
      "url": "{{baseUrl}}/api/items/{{itemId}}",
      "headers": [
        { "key": "Authorization", "value": "Bearer {{token}}" }
      ]
    }
  ]
}
